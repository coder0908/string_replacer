{
        "configurations": [
                {
                        "name": "C/C++: gcc build and debug active file",
                        "type": "cppdbg",
                        "request": "launch",
                        "program": "${fileDirname}/replacer",
                        "args": ["-l", "list.txt", "-t", "dummy.txt", "-r", "hello", "-n", "hi", "-s", "i", "-e", "hkle", "-E", "-S", "-c", "23", "-j", "14", "-t", "hi.txt", "-r", "sdfkjl", "-s", "sdfjlksd"],
                        "stopAtEntry": false,
                        "cwd": "${fileDirname}",
                        "environment": [],
                        "externalConsole": false,
                        "MIMode": "gdb",
                        "setupCommands": [
                                {
                                        "description": "Enable pretty-printing for gdb",
                                        "text": "-enable-pretty-printing",
                                        "ignoreFailures": true
                                },
                                {
                                        "description": "Set Disassembly Flavor to Intel",
                                        "text": "-gdb-set disassembly-flavor intel",
                                        "ignoreFailures": true
                                }
                        ],
                        "preLaunchTask": "C/C++: gcc build active file",
                        "miDebuggerPath": "/usr/bin/gdb"
                }
        ],
        "version": "2.0.0"
}